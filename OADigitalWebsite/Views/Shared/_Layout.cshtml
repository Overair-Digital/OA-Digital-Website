<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] Overair Digital</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/OADigitalWebsite.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <!-- Font Awesome -->
    <!-- Font Awesome -->
    
</head>
<body>
<div class="bg"></div>
<nav>
    <img class="logoImage" src="Images/overair.svg" alt="Overair Digital Logo"/>
    <input type="checkbox" id="click">
    <label for="click" class="menu-btn">
        <i class="fas fa-bars"></i>
    </label>
    <ul>
        <li><a class="active" asp-action="Index" asp-controller="Home">Home</a></li>
        <li><a href="https://overair.co.uk" target="_blank">Overair</a></li>
        <li><a asp-action="Contact" asp-controller="Home">Contact</a></li>
    </ul>
</nav>
<main role="main" class="pb-3">
    @RenderBody()
</main>
